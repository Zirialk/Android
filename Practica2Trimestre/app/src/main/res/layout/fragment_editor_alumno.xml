<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView
        android:id="@+id/imgFoto"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:contentDescription="@string/contentImgFoto"
        android:src="@drawable/icon_user_default"
        android:scaleType="centerCrop"
        />

    <ImageView
        android:id="@+id/imgNombre"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_person_black"
        android:layout_below="@id/imgFoto"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/paddingTopMaterialList"/>
    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/imgFoto"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="text" />
    <ImageView
        android:id="@+id/imgTelefono"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_phone_black_24dp"
        android:layout_below="@id/imgNombre"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/iconCreadorSeparacion"/>

    <EditText
        android:id="@+id/txtTelefono"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtNombre"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="phone" />
    <ImageView
        android:id="@+id/imgEmail"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_email_black_24dp"
        android:layout_below="@id/imgTelefono"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/iconCreadorSeparacion"/>
    <EditText
        android:id="@+id/txtEmail"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtTelefono"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="textEmailAddress" />
    <ImageView
        android:id="@+id/imgEmpresa"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_business_black_18dp"
        android:layout_below="@id/imgEmail"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/iconCreadorSeparacion"/>
    <EditText
        android:id="@+id/txtEmpresa"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtEmail"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="text" />
    <ImageView
        android:id="@+id/imgTutor"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_school_black_24dp"
        android:layout_below="@id/imgEmpresa"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/iconCreadorSeparacion"/>
    <EditText
        android:id="@+id/txtTutor"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtEmpresa"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="text" />
    <ImageView
    android:id="@+id/imgHorario"
    android:layout_width="@dimen/iconMaterialBounds"
    android:layout_height="@dimen/iconMaterialBounds"
    android:src="@drawable/ic_access_time_black_24dp"
    android:layout_below="@id/imgTutor"
    android:layout_marginLeft="@dimen/marginLeftMaterialList"
    android:layout_marginStart="@dimen/marginLeftMaterialList"
    android:layout_marginTop="@dimen/iconCreadorSeparacion"/>
    <EditText
        android:id="@+id/txtHorario"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtTutor"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="text" />
    <ImageView
        android:id="@+id/imgDireccion"
        android:layout_width="@dimen/iconMaterialBounds"
        android:layout_height="@dimen/iconMaterialBounds"
        android:src="@drawable/ic_location_on_black_24dp"
        android:layout_below="@id/imgHorario"
        android:layout_marginLeft="@dimen/marginLeftMaterialList"
        android:layout_marginStart="@dimen/marginLeftMaterialList"
        android:layout_marginTop="@dimen/iconCreadorSeparacion"/>
    <EditText
        android:id="@+id/txtDireccion"
        android:layout_width="@dimen/widthTxtCreador"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textSizeMaterialEditText"
        android:layout_below="@id/txtHorario"
        android:layout_marginLeft="@dimen/marginLeftItemAlumno"
        android:layout_marginStart="@dimen/marginLeftItemAlumno"
        android:inputType="text" />

</RelativeLayout>